{
  "name": "chatbot",
  "version": "1.0.0",
  "electronVersion": "1.8.1",
  "description": "A simple chatbot client app make with electron",
  "productName": "chatbot",
  "main": "main.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "gan": "node ./render/gan.js",
    "gan-debug": "set DEBUG = \"*\" && node ./render/gan.js",
    "package": "npm-run-all pack:*",
    "pack-win-mac": "electron-packager ./ chatbot --out ../dist --overwrite --asar --platform=win32,darwin --arch=x64,ia32 --ignore=./appData --icon=./img/icon/icon.ico",
    "pack-all": "electron-packager ./ --out ../dist --all --overwrite --asar --ignore=./appData --icon=./img/icon/icon.ico",
    "pack:win64": "electron-packager ./ --out ../dist --overwrite --asar --ignore=./appData --icon=./img/icon/icon.ico",
    "pack:win32": "electron-packager ./ --out ../dist --overwrite --ignore=./appData --icon=./img/icon/icon.ico --platform=win32 --arch=ia32",
    "pack:mac": "electron-packager ./ --out ../dist --overwrite --asar --ignore=./appData --icon=./img/icon/icon.ico --platform=darwin --arch=x64",
    "winstall": "node winstall.js"
  },
  "repository": "https://github.com/chunwei/chatbot",
  "bugs": {
    "url": "https://github.com/Microsoft/chatbot/issues"
  },
  "keywords": [
    "chatbot",
    "bot",
    "chat",
    "ai"
  ],
  "author": "luchunwei",
  "license": "MIT",
  "dependencies": {
    "baidu-aip-sdk": "^1.4.0",
    "debug": "^3.1.0",
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.8.1",
    "electron-packager": "^9.1.0",
    "electron-winstaller": "^2.6.3",
    "eslint": "^4.9.0",
    "eslint-config-google": "^0.9.1",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "npm-run-all": "^4.1.1"
  },
  "build": {
    "appId": "chatbot",
    "copyright": "RSVP",
    "productName": "chatbot",
    "win": {
      "icon": "img/icon/icon.ico"
    }
  }
}
